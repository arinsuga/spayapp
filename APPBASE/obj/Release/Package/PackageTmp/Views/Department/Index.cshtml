@model IEnumerable<APPBASE.Models.Department>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LOV_SEQ1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LOV_NM)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LOV_SEQ1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LOV_NM)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RUID }) |
            @Html.ActionLink("Details", "Details", new { id=item.RUID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RUID })
        </td>
    </tr>
}

</table>
